#include <stdio.h>

int main()
{
    int N, i, j, counter;
    printf("Привет, я программа и я умею выводить простые числа от 2 до заданного тобой числа. Чтобы у нас с тобой всё получилось, ты должен ввести натуральное число.\n");
    printf("Введите число: ");
    scanf ("%d", &N);
    printf("Спасибо, вот простые числа до ...  \n"); 
    {
        counter = 0;
        
        for (j = 1; i<= N; j++)
        {printf ("%d\n", counter);
            if (N%j == 0)
                counter++;
        }
        if (counter == 2)
            printf ("%d\n", i);
    }

    return 0;
}
